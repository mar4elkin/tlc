            <hr class="my-4">
            <% for (var i=0; i < tarifs.length; i++) {%>
                <h6><%= tarifs[i]%></h6>
                <div>
                    <table class="table table-dark" style="width: 100%; max-width: 100%; overflow-y : scroll; display:block;">
                        <thead>
                            <tr>
                                <th scope="row">#</th>
                                <% for (var j=0; j < place.length; j++) {%>
                                    <th scope="col" style="font-size: small;"><%= place[j]%> </th>
                                <%} %>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">Цена</th>
                                <% 
                                    var priceSelected = 0;
                                    if (i == 0) {
                                        priceSelected = priceLowcost;
                                    }

                                    if (i == 1) {
                                        priceSelected = priceComfort;
                                    }

                                    if (i == 2) {
                                        priceSelected = priceBisnes;
                                    }

                                    if (i == 3) {
                                        priceSelected = priceMiniven;
                                    }

                                    if (i == 4) {
                                        priceSelected = priceVip;
                                    }

                                    for (var z=0; z < priceSelected.length; z++) {%>
                                    <td scope="col" style="font-size: small;"><%= priceSelected[z]%> </td>
                                <%} %>
                            </tr>
                            
                        </tbody>
                    </table>
                </div>
            <%} %>
            
        </div>
